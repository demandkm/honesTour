<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.honestour.honestour.LoggedInPageProsp">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:lineSpacingExtra="24sp"
        android:text="Welcome honesTourGuide!"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="35dp" />

    <Button
        android:id="@+id/button"
        style="@android:style/Widget.Holo.Button"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:text="Scheduled Tours"
        android:textAlignment="viewStart"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="79dp" />

    <Button
        android:id="@+id/button2"
        style="@android:style/Widget.Holo.Button"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:text="Messages"
        android:textAlignment="viewStart"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="127dp" />

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="24dp"></LinearLayout>

    <Button
        android:id="@+id/button3"
        style="@android:style/Widget.Holo.Button"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:text="Payment History"
        android:textAlignment="viewStart"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="222dp" />

    <Button
        android:id="@+id/button4"
        style="@android:style/Widget.Holo.Button"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:text="Reviews"
        android:textAlignment="viewStart"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="174dp" />

    <Button
        android:id="@+id/button5"
        style="@android:style/Widget.Holo.Button"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:text="Logout"
        android:textAlignment="viewStart"
        android:textAppearance="@android:style/TextAppearance.Material.Large"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="270dp" />

    <ToggleButton
        android:id="@+id/toggleButton"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:checked="true"
        android:text="Availablity"
        android:textOff="Inactive"
        android:textOn="Active"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="318dp" />

</android.support.constraint.ConstraintLayout>
